<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.xmlns.jcp.org/xml/ns/javaee" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="3.1">
  <servlet>
    <servlet-name>reqServer</servlet-name>
    <servlet-class>com.cleannest.ReqServer</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>adminServer</servlet-name>
    <servlet-class>com.cleannest.AdminServer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>reqServer</servlet-name>
    <url-pattern>/reqing</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>adminServer</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
</web-app>